<div class="login-wrapper">
    <form class="form-signin" id="signInForm">
        <h1><img src="assets/logo_04.png" alt="" width ="100px" height = "100px" /></h1>
        <h2 class="form-signin-heading">Please login</h2>
        <div id="signInError" role="alert" class="alert hidden"></div>
        <div class="form-group">
            <input type="text" class="form-control" name="email" id="email" placeholder="Email Address" required autofocus />
        </div>
        <div class="form-group">
            <input type="password" class="form-control" name="password" id="password" value="" placeholder="Password" required/>
        </div>

        <button class="btn btn-lg btn-blue btn-block" type="submit" style=" background:#8B0000">Login</button>
        <p class="marTop20">New Here? <a href="/member/register" class="">click here to Register</a></p>
    </form>
</div>

<script src="/assets/login.js" type="text/javascript"></script>


